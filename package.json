{
  "name": "butler",
  "version": "0.0.1-beta.1",
  "description": "Generate a dependency tree",
  "main": "dist/tree.cjs.js",
  "module": "dist/tree.esm-bundler.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "dev": "http-server -p 8085 ./",
    "format": "prettier --write --parser typescript \"(src|tests)/**/*.(js|ts?(x))\""
  },
  "author": "imtaotao",
  "keywords": [
    "npm",
    "tree",
    "butler"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imtaotao/butler.git"
  },
  "bugs": {
    "url": "https://github.com/imtaotao/butler/issues"
  },
  "dependencies": {
    "esm-semver": "^0.0.9",
    "gpi": "^0.0.6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/node": "^18.7.2",
    "prettier": "^2.5.1",
    "rollup": "^2.67.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "^4.5.5"
  }
}
